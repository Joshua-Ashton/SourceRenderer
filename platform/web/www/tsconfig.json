{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es2015",
    "target": "ES2017",
    "jsx": "react",
    "allowJs": true,
    "lib": ["ES2017", "dom"],
    "moduleResolution": "node",
    "strict": true,
    "sourceMap": true,
    "removeComments": true,
    "rootDir": ".",
    "rootDirs": [".", "./dist"]
  },
  "exclude": [
    "./src/service_worker.ts",
    "./dist/"
  ]
}
